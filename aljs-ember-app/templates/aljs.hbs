<header class="site-banner" role="banner"><a class="active" href="/"><img src="images/aljs-logo.svg"></a>
</header>
<main class="site-main" role="main">
    <div class="site-content">
        <header class="site-masthead slds-grid">
            <div class="site-masthead-title slds-col slds-has-flexi-truncate slds-align-middle">
                <div class="slds-media__body">
                    <div class="slds-grid">
                        <h1 title="Appiphony Lightning JS" class="slds-p-left--large">Appiphony Lightning JS</h1></div>
                </div>
            </div>
        </header>
        <div class="site-content slds-p-around--xx-large site-design site-design-layout">
            {{outlet}}
        </div>
    </div>
</main>
<nav id="navigation" class="site-navigation" role="navigation">
    <ul class="slds-list--vertical slds-has-block-links">
        {{#each section in sections}}
            <li class="slds-list__item">{{#link-to section.path}}{{section.name}}{{/link-to}}</li>
        {{/each}}
    </ul>
</nav>
<footer class="site-contentinfo slds-wrap site-text-longform slds-text-body--small" role="contentinfo">
